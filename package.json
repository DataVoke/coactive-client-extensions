{
  "name": "coactive-client-extensions",
  "version": "0.0.3",
  "description": "Coactive client extensions",
  "main": "lib/index.js",
  "scripts": {
    "test": "(npm run build && mocha --require @babel/register)",
    "build": "babel src -d lib --source-maps true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataVoke/coactive-client-extensions.git"
  },
  "keywords": [
    "coactive",
    "client",
    "extensions"
  ],
  "author": "Coactive, Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DataVoke/coactive-client-extensions/issues"
  },
  "homepage": "https://github.com/DataVoke/coactive-client-extensions#readme",
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/babel@7.0.1",
      "tester": "bit.envs/testers/mocha@5.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "overrides": {
      "coactive.coactive-client-extensions/load-extension": {}
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "mocha": "^6.2.1"
  },
  "dependencies": {}
}
